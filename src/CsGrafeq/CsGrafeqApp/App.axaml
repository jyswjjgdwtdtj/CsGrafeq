<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CsGrafeqApp.App"
             RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Controls/CheckedControl.axaml" />
                <ResourceInclude Source="/Controls/DisplayerContainer.axaml" />
                <ResourceInclude Source="/Resources/ColorResource.axaml" />
                <ResourceInclude Source="/Themes/BackgroundTheme.axaml" />
                <ResourceInclude Source="/Controls/ResizableSplitter.axaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
</Application>