<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType Condition="'$(Configuration.toUpper())' != 'DEBUG'">WinExe</OutputType>
        <OutputType Condition="'$(Configuration.toUpper())' == 'DEBUG'">Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <InvariantGlobalization>false</InvariantGlobalization>
        <Nullable>enable</Nullable>
        <PublishAot Condition="'$(Configuration.toUpper())' != 'DEBUG'">true</PublishAot>
        <PublishAot Condition="'$(Configuration.toUpper())' == 'DEBUG'">false</PublishAot>
        <AvaloniaUseCompiledBindingsByDefault>true</AvaloniaUseCompiledBindingsByDefault>
        <LangVersion>Preview</LangVersion>
    </PropertyGroup>

    <PropertyGroup>
        <ApplicationManifest>app.manifest</ApplicationManifest>
        <ApplicationIcon>favicon.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\CsGrafeqApplication\CsGrafeqApplication.csproj"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Remove="Packaging.Targets"/>
    </ItemGroup>

    <ItemGroup>
        <Content Include="favicon.ico"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Avalonia.Desktop"/>
        <!--<PackageReference Include="PublishAotCompressed"/>-->
	    <!--会报毒。。。-->
    </ItemGroup>
</Project>