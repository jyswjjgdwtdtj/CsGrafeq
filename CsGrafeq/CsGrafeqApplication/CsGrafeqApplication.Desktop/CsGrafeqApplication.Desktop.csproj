<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <OutputType Condition="'$(Configuration.toUpper())' != 'DEBUG'">WinExe</OutputType>
	  <OutputType Condition="'$(Configuration.toUpper())' == 'DEBUG'">Exe</OutputType>
	  <TargetFramework>net9.0</TargetFramework>
	  <InvariantGlobalization>true</InvariantGlobalization>
	  <Nullable>enable</Nullable>
	  <PublishAot>true</PublishAot>
	  <RuntimeIdentifier>win-x64</RuntimeIdentifier>
	  <PublishTrimmed>True</PublishTrimmed>
	  <AvaloniaUseCompiledBindingsByDefault>true</AvaloniaUseCompiledBindingsByDefault>
	  <LangVersion>Preview</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\CsGrafeqApplication\CsGrafeqApplication.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Remove="Packaging.Targets" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Avalonia.Desktop" />
  </ItemGroup>
</Project>