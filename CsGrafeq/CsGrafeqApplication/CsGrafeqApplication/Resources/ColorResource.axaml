<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:CsGrafeqApplication.ValueConverter"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <system:UInt32 x:Key="MedianUint">4278237343</system:UInt32>
    <system:UInt32 x:Key="LightUint">8978380</system:UInt32>
    <system:UInt32 x:Key="DarkUint">32896</system:UInt32>
    <Color x:Key="Median">#00b89f</Color>
    <Color x:Key="Light">#88ffcc</Color>
    <Color x:Key="Dark">Teal</Color>
    <SolidColorBrush x:Key="HTMedianBrush" Color="{DynamicResource Median}" Opacity="0.3" />
    <SolidColorBrush x:Key="HTLightBrush" Color="{DynamicResource Light}" Opacity="0.3" />
    <SolidColorBrush x:Key="MedianBrush" Color="{DynamicResource Median}" />
    <SolidColorBrush x:Key="LightBrush" Color="{DynamicResource Light}" />
    <SolidColorBrush x:Key="HTDarkBrush" Color="{DynamicResource Dark}" Opacity="0.3" />
    <SolidColorBrush x:Key="DarkBrush" Color="{DynamicResource Dark}" />
    <converter:UIntColorToBrush x:Key="UIntColorToBrush" />
    <converter:UIntToColor x:Key="UIntToColor" />
</ResourceDictionary>