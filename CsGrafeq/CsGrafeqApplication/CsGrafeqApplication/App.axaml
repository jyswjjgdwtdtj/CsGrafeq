<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dialogHostAvalonia="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
             xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             x:Class="CsGrafeqApplication.App"
             RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    <Application.Styles>
        <themes:MaterialTheme BaseTheme="Light" PrimaryColor="Green" SecondaryColor="Amber"></themes:MaterialTheme>
        <dialogHostAvalonia:DialogHostStyles />
    </Application.Styles>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Controls/CheckedControl.axaml" />
                <ResourceInclude Source="/Resources/ColorResource.axaml" />
                <ResourceInclude Source="/Themes/BackgroundTheme.axaml" />
                <ResourceInclude Source="/Resources/Resource.axaml" />
                <ResourceInclude Source="/Controls/GlobalMathKeyBoard.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <FontFamily x:Key="MapleMono">avares://CsGrafeqApplication/Fonts/MapleMono-CN-Regular.ttf#Maple Mono CN</FontFamily>
    </Application.Resources>
    <Application.Styles>
        <StyleInclude Source="/Controls/ShownControl.axaml" />
        <StyleInclude Source="/Controls/KeyButton.axaml" />
        <StyleInclude Source="/Controls/MathTextBox.axaml"/>
        <StyleInclude Source="/Controls/VariableBox.axaml"/>
        
    </Application.Styles>
</Application>